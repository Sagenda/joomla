<?xml version="1.0" encoding="utf-8"?>
<extension type="module" version="3.1.0" client="site" method="upgrade">
    <name>Sagenda Calendar</name>
    <author>Iteration.info SARL</author>
    <version>0.0.1</version>
    <description>A free Booking Calendar module</description>
    <files>
        <filename>mod_sagenda_calendar.xml</filename>
        <filename module="mod_sagenda_calendar">mod_sagenda_calendar.php</filename>
        <filename>index.html</filename>
        <filename>helper.php</filename>
        <filename>tmpl/default.php</filename>
        <filename>tmpl/index.html</filename>
        <filename>tmpl/bootstrap-theme.css.map</filename>
        <filename>tmpl/bootstrap-theme-wrapper.css</filename>
        <filename>tmpl/bootstrap-wrapper.css</filename>
        <filename>tmpl/main.js</filename>
        <filename>tmpl/polyfills.js</filename>
        <filename>tmpl/runtime.js</filename>
        <filename>tmpl/styles.css</filename>
        <folder>tmpl/assets/</folder>
    </files>
    <config>
        <fields name="params">
            <fieldset name="basic">
                <field
                    name="sagenda_token"
                    type="text"
                    label="Token"
                    description=""
                    size="60"
                    default=""
                />
            </fieldset>
        </fields>
    </config>
</extension>